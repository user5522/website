{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"bracketSameLine": true,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"tailwindConfig": "tailwind.config.cjs",
	"pluginSearchDirs": false,
	"overrides": [
		{
			"files": ["*.svelte"],
			"options": {
				"parser": "svelte"
			}
		}
	]
}
